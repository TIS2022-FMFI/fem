<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ScrollPane?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0"
            prefWidth="500.0" style="-fx-border-color: #000000; -fx-border-width: 10; -fx-background-color: #FFFDD0;"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="main.ui.KeywordsController">
        <HBox prefHeight="100.0" prefWidth="500.0">
            <Label alignment="CENTER" text="ALL KEYWORDS">
                <font>
                    <Font name="Calibri Bold" size="35.0"/>
                </font>
                <padding>
                    <Insets left="130.0" top="35.0"/>
                </padding>
            </Label>
        </HBox>
        <ScrollPane pannable="true" layoutY="90.0" layoutX="115.0" prefHeight="100.0" prefWidth="250.0">
            <VBox fx:id="keywordList">
            </VBox>
        </ScrollPane>
        <VBox layoutY="210.0" layoutX="115.0" prefHeight="100.0" prefWidth="500.0">
            <Button fx:id="closeButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="250.0"
                    style="-fx-background-radius: 10; -fx-border-radius: 10; -fx-border-color: #000000; -fx-border-width: 2; -fx-background-color: #FE2C54;"
